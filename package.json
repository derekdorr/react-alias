{
  "name": "react-alias",
  "version": "1.0.3",
  "description": "Basic HTML components aliased in React in an effort to cutdown on strings",
  "repository": "https://github.com/derekdorr/react-alias",
  "main": "lib/index.js",
  "modules": "es/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "es",
    "src",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "docs": "gulp docs",
    "test": "cross-env BABEL_ENV=lib nyc tape tests/*-test.js",
    "lint": "eslint --fix src/ tests/",
    "build-browser": "rollem",
    "build-lib": "cross-env BABEL_ENV=lib babel src --out-dir lib",
    "build-es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build-lib && npm run build-es && npm run build-browser",
    "precommit": "npm run lint && npm run test"
  },
  "nyc": {
    "exclude": [
      "tests/"
    ],
    "require": [
      "babel-register",
      "./tests/.setup.js"
    ]
  },
  "author": "Derek Dorr",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-jsdoc-to-markdown": "^1.2.2",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "husky": "^0.14.3",
    "nyc": "^11.3.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "rollem": "^1.12.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.2",
    "sinon": "^4.1.3",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "permanent": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": ">=15.0.0",
    "skald": "^1.12.0"
  }
}
